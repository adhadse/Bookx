<?xml version="1.0" encoding="utf-8"?>
<schemalist>
  <schema path="/com/adhadse/Bookx/" id="@APP_ID@" gettext-domain="@PKGNAME@">
    <key name="window-width" type="i">
      <default>950</default>
      <summary>Window width</summary>
    </key>
    <key name="window-height" type="i">
      <default>650</default>
      <summary>Window height</summary>
    </key>
    <key name="bookx-reader-window-width" type="i">
      <default>1050</default>
      <summary>Bookx Reader Window width</summary>
    </key>
    <key name="bookx-reader-window-height" type="i">
      <default>720</default>
      <summary>Bookx Reader Window height</summary>
    </key>
    <key name="window-previous-width" type="i">
      <default>950</default>
    </key>
    <key name="window-previous-height" type="i">
      <default>650</default>
    </key>
    <key name="is-maximized" type="b">
      <default>false</default>
      <summary>Window maximized state</summary>
    </key>
    <key name="is-bookx-reader-maximized" type="b">
      <default>true</default>
      <summary>Bookx Reader Window maximized state</summary>
    </key>
    <key name="dark-mode" type="b">
      <default>true</default>
    </key>
    <key name="books-dir" type="s">
      <default>''</default>
      <summary>Load books from folder</summary>
    </key>
  </schema>
</schemalist>
